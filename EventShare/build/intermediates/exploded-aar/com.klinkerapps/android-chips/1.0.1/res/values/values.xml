    <dimen name="chip_icon_margin_start">12dip</dimen>
    <dimen name="chip_padding">12dip</dimen>
    <dimen name="chip_subtitle_padding_start">0dip</dimen>
    <dimen name="chip_text_size">14sp</dimen>
    <dimen name="chip_title_padding_start">0dip</dimen>
    <dimen name="extra_chip_height">4dp</dimen>
    <dimen name="line_spacing_extra">4dip</dimen>
    <dimen name="padding_no_picture">60dp</dimen>

    <integer name="chips_max_lines">-1</integer>
